# Staging Environment Variables
# Phase 9: Staging deployment configuration

# Database Configuration
MYSQL_ROOT_PASSWORD=StagingRootPassword123!
MYSQL_PASSWORD=StagingPassword123!
MYSQL_DATABASE=QueueHubDb
MYSQL_USER=queuehub_user
MYSQL_HOST=mysql
MYSQL_PORT=3306

# JWT Configuration
JWT_SECRET_KEY=StagingJWTSecretKey123456789012345678901234567890
JWT_ISSUER=GrandeTech.QueueHub.Staging
JWT_AUDIENCE=GrandeTech.QueueHub.Staging
JWT_EXPIRATION_MINUTES=60

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=1000

# Data Anonymization
DATA_ANONYMIZATION_SALT=StagingDataAnonymizationSalt123456789012345678901234567890

# Application Insights
APPLICATION_INSIGHTS_CONNECTION_STRING=InstrumentationKey=your-staging-instrumentation-key;IngestionEndpoint=https://your-staging-region.in.applicationinsights.azure.com/

# Monitoring
GRAFANA_ADMIN_PASSWORD=StagingGrafanaPassword123!

# SSL/TLS (for production-like testing)
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem
